    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/pg": "^8.10.9",
    "@types/bcrypt": "^5.0.2",
    "@types/express-session": "^1.17.10",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cors": "^2.8.17",
